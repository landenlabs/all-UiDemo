<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TextAppearanceSwitch">
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textStyle" format="integer" />
        <attr name="typeface" format="reference" />
        <attr name="fontFamily" format="string" />
        <attr name="underline" format="boolean" />
        <attr name="descenders" format="boolean" />
        <attr name="textColorHighlight" format="color" />
        <attr name="textColorHint" format="color" />
        <attr name="textColorLink" format="color" />
    </declare-styleable>


    <attr name="CompatSeekBar" format="reference" />
    <declare-styleable name="CompatSeekBar">

        <attr name="tickMin" format="integer" />
        <attr name="tickMax" format="integer" />
        <attr name="tickStep" format="integer" />
        <attr name="tickWidth" format="integer" />
        <attr name="showTick" format="boolean" />
        <attr name="tickColor" format="color" />
        <attr name="tickUnder" format="boolean" />

        <!-- Warning attribute "tickMark" is already used by SeekBar so use tickMarker -->
        <attr name="tickMarker" format="reference" />
    </declare-styleable>


    <declare-styleable name="BoundedLinearLayout">
        <attr name="width" format="dimension" />
        <attr name="height" format="dimension" />
        <attr name="widthPercent" format="float" />
        <attr name="heightPercent" format="float" />
        <attr name="enabled" format="boolean" />
        <attr name="aspectWdivH" format="float" />
        <attr name="include" format="reference" />
    </declare-styleable>

    <attr name="gravityItems">
        <flag name="bottom" value="80" />
        <flag name="center" value="17" />
        <flag name="center_horizontal" value="1" />
        <flag name="center_vertical" value="16" />
        <flag name="clip_horizontal" value="8" />
        <flag name="clip_vertical" value="128" />
        <flag name="end" value="8388613" />
        <flag name="fill" value="119" />
        <flag name="fill_horizontal" value="7" />
        <flag name="fill_vertical" value="112" />
        <flag name="left" value="3" />
        <flag name="right" value="5" />
        <flag name="start" value="8388611" />
        <flag name="top" value="48" />
    </attr>

    <attr name="widthItems" format="dimension">
        <enum name="fill_parent" value="-1" />
        <enum name="match_parent" value="-1" />
        <enum name="wrap_content" value="-2" />
    </attr>

    <attr name="wheel" format="boolean" />
    <attr name="visibleItems" format="integer" />

    <declare-styleable name="Picker1">
        <attr name="android:entries" />
        <attr name="android:gravity" />
        <attr name="wheel" />
        <attr name="gravityItems" />
        <attr name="visibleItems" />
        <attr name="widthItems" />
    </declare-styleable>

    <declare-styleable name="Picker2">
        <attr name="android:entries" />
        <attr name="android:gravity" />
        <attr name="wheel" />
        <attr name="gravityItems" />
        <attr name="visibleItems" />
        <attr name="widthItems" />
    </declare-styleable>

</resources>